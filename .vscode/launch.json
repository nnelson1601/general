    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug app",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/app.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Debug Filterer",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/filterer.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Attach to Python Functions",
            "type": "python",
            "request": "attach",
            "port": 9091,
            "preLaunchTask": "func: host start"
        }
    ]
}